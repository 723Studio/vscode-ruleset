{
  "$id": "https://openxcom.org/schemas/oxc/ExtraSounds.json",
  "$schema": "http://json-schema.org/draft-07/schema#",
  "type": "object",
  "additionalProperties": false,
  "required": [
    "type"
  ],
  "properties": {
    "type": {
      "type": "string",
      "enum": [
        "GEO.CAT",
        "BATTLE.CAT",
        "INTRO.CAT"
      ]
    },
    "files": {
      "patternProperties": {
        "^\\d+$": {
          "type": "string"
        }
      },
      "additionalProperties": false,
      "type": "object"
    }
  }
}
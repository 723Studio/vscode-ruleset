{
  "$merge": {
    "source": {
      "$import": "../oxc/RuleTerrain.json"
    },
    "with": {
      "$id": "https://openxcom.org/schemas/oxce/RuleTerrain.json",
      "properties": {
        "ambienceRandom": {
          "$ref": "ModOffset.json#/definitions/List"
        },
        "ambienceRandomDelay": {
          "items": {
            "type": "integer"
          },
          "minItems": 2,
          "maxItems": 2,
          "type": "array"
        },
        "addOnly": {
          "type": "boolean"
        },
        "enviroEffects": {
          "type": "string"
        }
      }
    }
  }
}